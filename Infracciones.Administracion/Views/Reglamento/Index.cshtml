@model IEnumerable<Infracciones.Dto.Reglamento>

@{
    ViewBag.Title = "Reglamento";
}

<div class="container-fluid">
    <h2>Reglamento</h2>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Multa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDescuento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDeposito)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Isactivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Articulo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fraccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Motivo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaDeRegistro)
            </th>>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Multa)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsDescuento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsDeposito)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Isactivo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Articulo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Fraccion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Motivo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaDeRegistro)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>

</div>